@page
@model Asp_Identity.Pages.LogoutModel
@{
}

<form method="post">
    <div class="container mt-5">
        <div class="row justify-content-center">
            <div class="col-sl-12 col-md-12 col-lg-4">
                <p> Are you Sure you Want to Logout</p>
                <p>
                    <button class="btn btn-primary" asp-page-handler="Logout">Logout</button>
                    <button class="btn btn-light ml-5 border-dark" asp-page-handler="NoLogout">Stay In</button>
                </p>
            </div>
        </div>
    </div>
</form>